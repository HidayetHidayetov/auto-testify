{
    "name": "hidayetov/auto-testify",
    "description": "Automatic test generator for Laravel models",
    "type": "laravel-package",
    "version": "1.0.1",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Hidayetov\\AutoTestify\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Hidayet Hidayetov",
            "email": "hidayet.hidayetov@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.1",
        "laravel/framework": "^11.0"
    },
    "suggest": {
        "doctrine/dbal": "Required for detecting unique fields from the database schema (^3.0)"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hidayetov\\AutoTestify\\AutoTestifyServiceProvider"
            ]
        }
    }
}
